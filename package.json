{
    "name": "sweet-modal-vue-3",
    "description": "SweetModal library for use with Vue 3, Nuxt 3",
    "author": {
        "name": "Festus Iyere"
    },
    "version": "0.4.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/festusiyere/sweet-modal-vue-3.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/@festusiyere"
    },
    "type": "module",
    "keywords": [
        "sweetmodal",
        "sweet-modal",
        "modal",
        "vue",
        "vite",
        "vue 3",
        "nuxt 3",
        "nuxt3",
        "vue3",
        "library"
    ],
    "main": "./dist/sweet-modal-vue-3.umd.js",
    "module": "./dist/sweet-modal-vue-3.es.js",
    "exports": {
        ".": {
            "import": "./dist/sweet-modal-vue-3.es.js",
            "require": "./dist/sweet-modal-vue-3.umd.js"
        }
    },
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
        "preserve": "vite build",
        "serve": "vite preview --port 5050",
        "typecheck": "vue-tsc --noEmit",
        "preview": "vite preview",
        "test": "exit 0"
    },
    "dependencies": {
        "vue": "^3.2.45"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "@vitejs/plugin-vue": "^4.0.0",
        "sass": "^1.79.2",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-css-injected-by-js": "^2.4.0",
        "vue-tsc": "^1.0.11"
    }
}
