{
    "name": "sweet-modal-vue-3",
    "description": "SweetModal library for use with Vue 3, Nuxt 3",
    "author": {
        "name": "Festus Iyere"
    },
    "version": "0.2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/festus-iyere/sweet-modal-vue-3.git"
    },
    "keywords": [
        "sweetmodal",
        "sweet-modal",
        "modal",
        "vue",
        "vite",
        "vue 3",
        "nuxt 3",
        "nuxt3",
        "vue3",
        "library"
    ],
    "main": "./dist/sweet-modal-vue-3.cjs",
    "module": "./dist/sweet-modal-vue-3.js",
    "exports": {
        ".": "./dist/sweet-modal-vue-3.js"
    },
    "types": "./dist/SweetModal.vue.d.ts",
    "files": [
        "dist"
    ],
    "license": "ISC",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "vite-plugin-css-injected-by-js": "^2.2.0",
        "vue": "^3.2.45"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/jsdom": "^20.0.1",
        "@types/node": "^18.11.12",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.2.6",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "rollup-plugin-typescript2": "^0.34.1",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "typescript": "~4.7.4",
        "vite": "^4.0.0",
        "vitest": "^0.25.6",
        "vue-tsc": "^1.0.12"
    }
}
